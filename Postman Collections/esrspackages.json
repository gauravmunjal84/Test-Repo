      "_postman_id": "017398f8-1ffd-8159-68f5-1f96ab505dfe",
      "name": "GET esrs upgrade package schedule  ",
      "event": [
        {
          "listen": "test",
          "script": {
            "type": "text/javascript",
            "exec": [
              "var data = JSON.parse(responseBody);",
              "tests[\"Status code is 200\"] = responseCode.code === 200;",
              "if(responseCode.code === 200){",
              "postman.setEnvironmentVariable(\"schIdNotify\", data.content[0].id);",
              "tests[\"Schedule Name\"] = data.content[0].name === \"ESRSUpgradePackage\";",
              "tests[\"Verify schedule is enabled\"] = data.content[0].enabled === true;",
              "tests[\"Verify Hourly Trigger\"] = data.content[0].triggers[0].hourlyFrequency === 24;",
              "}",
              "",
              "",
              "",
              "",
              "",
              ""
            ]
          }
        }
      ],
      "request": {
        "url": "https://{{ip}}:8443/api/v1/schedules?filter=name eq \"ESRSUpgradePackage\"",
        "method": "GET",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json",
            "description": ""
          },
          {
            "key": "X-Auth-Token",
            "value": "{{token}}",
            "description": ""
          }
        ],
        "body": {},
        "description": "GET ESRS notification schedule for upgrade package"
      },
      "response": []
