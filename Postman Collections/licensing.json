            "type": "text/javascript",
            "exec": [
              "var data = JSON.parse(responseBody);",
              "tests[\"Status code is 200\"] = responseCode.code === 200;",
              "if(responseCode.code === 200){",
              "postman.setEnvironmentVariable(\"licenseId\", data.content[0].id);",
              "tests[\"type before adding license\"] = data.content[0].type === \"TRIAL\";",
              "tests[\"status before adding license\"] = data.content[0].status === \"VALID\";",
