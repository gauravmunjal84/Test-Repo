      "name": "GET notifications @after GET upgradePackages",
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "type": "text/javascript",
            "exec": [
              "",
              ""
            ]
          }
        },
        {
          "listen": "test",
          "script": {
            "type": "text/javascript",
            "exec": [
              "var data = JSON.parse(responseBody);",
              "",
              "tests[\"Verify published notification is unacknowledged by default\"] = data.content[0].ack.ackState === \"UNACK\";",
              "",
              "tests[\"Verify the severity of notification\"] = data.content[0].severity === \"NOTIFICATION\";",
              "",
              "tests[\"Verify the category of notification\"] = data.content[0].category === \"MONITORING\";"
            ]
          }
        }
      ],
      "request": {
        "method": "GET",
